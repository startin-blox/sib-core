{
  "compilerOptions": {
    "target": "es2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "composite": false,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "*": ["web_modules/.types/*"]
    },
    "skipLibCheck": true,
    "types": ["cypress"]
  },
  "include": ["./src/**/*", "./cypress/**/*"],
  "exclude": ["./node_modules/", "./cypress.config.ts"]
}
