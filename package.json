{
  "name": "@startinblox/core",
  "version": "0.0.0",
  "description": "This is a series of web component respecting both the web components standards and the Linked Data Platform convention.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "directories": {
    "example": "examples"
  },
  "files": [
    "dist",
    "doc"
  ],
  "scripts": {
    "build": "snowpack build --polyfill-node",
    "watch": "npm run build -- --watch",
    "serve": "node server.js",
    "check-types": "tsc",
    "test": "node server.js --test",
    "cypress": "node server.js --test-ui"
  },
  "repository": {
    "type": "git",
    "url": "https://git.startinblox.com/framework/sib-core.git"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "major",
              "release": "major"
            },
            {
              "type": "minor",
              "release": "minor"
            },
            {
              "type": "*",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/gitlab",
      "@semantic-release/npm"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rckeller/cypress-unfetch": "^1.0.1",
    "@snowpack/plugin-babel": "^2.1.6",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@types/autolinker": "^2.0.0",
    "@types/leaflet": "^1.5.21",
    "@types/leaflet.markercluster": "^1.4.3",
    "@types/markdown-it": "^12.0.1",
    "@types/pubsub-js": "^1.8.2",
    "@types/quill": "^2.0.4",
    "autolinker": "^3.14.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-remove-import-js-extension": "0.0.3",
    "body-parser": "^1.19.0",
    "cypress": "^6.9.1",
    "cypress-iframe": "^1.0.1",
    "cypress-plugin-tab": "^1.0.5",
    "delta-markdown-for-quill": "0.0.9",
    "dialog-polyfill": "^0.5.6",
    "document-register-element": "^1.14.6",
    "express": "^4.17.1",
    "find-free-port": "^2.0.0",
    "fuse.js": "^6.4.6",
    "jsonld-context-parser": "^1.3.4",
    "leaflet": "1.7.1",
    "leaflet.markercluster": "1.4.1",
    "lit-html": "^1.3.0",
    "markdown-it": "^12.0.4",
    "markdown-it-link-attributes": "4.0.1",
    "pubsub-js": "^1.9.2",
    "quill": "^1.3.7",
    "quill-delta-to-markdown": "^0.6.0",
    "slim-select": "^1.27.0",
    "snowpack": "^3.0.11",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tui-calendar": "^1.13.0",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
